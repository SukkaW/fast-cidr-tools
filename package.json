{
  "name": "fast-cidr-tools",
  "version": "0.3.3",
  "description": "cidr-tools but up to 20x faster",
  "homepage": "https://github.com/SukkaW/fast-cidr-tools#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SukkaW/fast-cidr-tools.git"
  },
  "bugs": {
    "url": "https://github.com/SukkaW/fast-cidr-tools/issues"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.mjs",
  "types": "./dist/cjs/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/es/index.d.mts",
        "default": "./dist/es/index.mjs"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "lint": "eslint --format=sukka .",
    "build": "bunchee --clean --target=es2021",
    "test": "mocha --require @swc-node/register test/index.test.ts",
    "benchmark": "node -r @swc-node/register benchmark.cts",
    "prepublishOnly": "rimraf dist && npm run test && npm run build",
    "prerelease": "pnpm run lint && pnpm run test && pnpm run build",
    "release": "bumpp -r --all --commit \"release: %s\" --tag \"%s\""
  },
  "keywords": [
    "cidr",
    "ip",
    "ipv4"
  ],
  "author": "Sukka <https://skk.moe>",
  "license": "MIT",
  "dependencies": {
    "foxts": "^4.1.0"
  },
  "devDependencies": {
    "@eslint-sukka/node": "^7.2.1",
    "@swc-node/register": "^1.11.1",
    "@swc/core": "^1.13.5",
    "@types/mocha": "^10.0.10",
    "bumpp": "^10.2.3",
    "bunchee": "^6.6.0",
    "cidr-tools": "^11.0.3",
    "cidr-tools-wasm": "^0.0.14",
    "eslint": "^9.37.0",
    "eslint-config-sukka": "^7.2.1",
    "eslint-formatter-sukka": "^7.2.1",
    "expect": "^30.2.0",
    "mitata": "^1.0.34",
    "mocha": "^11.7.4",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.18.0",
  "engines": {
    "node": ">=16"
  },
  "pnpm": {
    "overrides": {
      "eslint>chalk": "npm:picocolors@^1.1.1",
      "is-core-module": "npm:@nolyfill/is-core-module@^1.0.39",
      "safe-buffer": "npm:@nolyfill/safe-buffer@^1.0.44"
    },
    "ignoredBuiltDependencies": [
      "unrs-resolver"
    ],
    "onlyBuiltDependencies": [
      "@swc/core",
      "oxc-resolver"
    ]
  }
}
